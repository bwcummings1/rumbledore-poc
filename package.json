{
  "name": "rumbledore",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:all": "npm run type-check && npm run lint && npm test",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:test:reset": "DATABASE_URL=$TEST_DATABASE_URL prisma migrate reset --force",
    "health:check": "tsx scripts/health-check.ts",
    "sprint:validate": "tsx scripts/sprint-validate.ts",
    "sprint:complete": "tsx scripts/sprint-complete.ts",
    "sprint:prerequisites": "tsx scripts/check-prerequisites.ts",
