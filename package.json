    "verify:setup": "tsx scripts/verify-setup.ts",
    "verify:env": "tsx scripts/verify-env.ts",
    "cache:clear": "redis-cli FLUSHALL",
    "bench": "tsx scripts/benchmark.ts",
    "security:check": "npm audit && tsx scripts/security-check.ts",
    "profile": "tsx scripts/profile.ts",
    "stats:worker": "tsx lib/workers/statistics-worker.ts",
    "stats:scheduler": "tsx lib/workers/statistics-scheduler.ts",
    "stats:init": "tsx scripts/initialize-statistics.ts",
    "stats:refresh-views": "tsx scripts/refresh-materialized-views.ts",
    "stats:calculate": "tsx scripts/calculate-statistics.ts",
    "stats:test": "jest __tests__/lib/stats --coverage",
    "stats:test:integration": "jest __tests__/integration/statistics-flow.test.ts",
    "stats:test:performance": "jest __tests__/performance/statistics-large-dataset.test.ts",
    "worker:start": "npm run stats:worker & npm run stats:scheduler",
    "worker:pm2:start": "pm2 start ecosystem.config.js",
    "worker:pm2:stop": "pm2 stop ecosystem.config.js",
    "worker:pm2:restart": "pm2 restart ecosystem.config.js",
    "worker:pm2:logs": "pm2 logs",
    "worker:pm2:status": "pm2 status",
    "admin:create": "tsx scripts/create-admin.ts"
